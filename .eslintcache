mn\">\n            {newsData.map((article, index) => {\n              if (index > 7 && index < 11) {\n                return (\n                  <ArticleSingle\n                    id={article.id}\n                    key={article.id}\n                    img={false}\n                    article={article}\n                  />\n                );\n              }\n            })}\n          </div>\n        </div>\n      )}\n    </>\n  );\n};\n\nexport default SubMain;\n","/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Grid/MostViewed.js",["228","229"],"import React from \"react\";\n\nimport { Link, useLocation } from \"react-router-dom\";\n\nconst MostViewed = (props) => {\n  const { newsData, loading } = props;\n  const location = useLocation();\n  const path = location.pathname;\n  return (\n    <>\n      {!loading && (\n        <div className=\"most-viewed\">\n          <ul className=\"grid__item--span1 most-viewed__list--left\">\n            {newsData.map((entry, index) => {\n              if (index >= 0 && index < 5) {\n                return (\n                  <Link\n                    to={\n                      path === \"/\"\n                        ? `article/${entry.id}`\n                        : `${path}/article/${entry.id}`\n                    }\n                    id={entry.id}\n                    key={entry.id}\n                  >\n                    <li className=\"most-viewed__list-item\">\n                      <p className=\"most-viewed__index-heading\">{index + 1} </p>\n                      <h4 className=\"most-viewed__heading\">\n                        {entry.fields.headline}\n                      </h4>\n                    </li>\n                  </Link>\n                );\n              }\n            })}\n          </ul>\n          <ul className=\"grid__item--span1  most-viewed__list--right\">\n            {newsData.map((entry, index) => {\n              if (index >= 5 && index < 10) {\n