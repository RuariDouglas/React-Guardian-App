/src/index.js",[],["162","163"],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/reportWebVitals.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/App.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Redux/Reducers/index.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/GlobalStyled.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Redux/Reducers/guardianReducers.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Redux/Reducers/menuToggleReducer.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Redux/Reducers/weatherReducer.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Nav/Nav.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Pages/Home.js",["164","165","166"],"// REACT\nimport React, { useEffect } from \"react\";\nimport { useLocation } from \"react-router-dom\";\n\n// REDUX\nimport { loadSection } from \"../Redux/Actions/guardianSectionAction\";\nimport { useDispatch, useSelector } from \"react-redux\";\n\n// COMPONENTS\nimport WeatherApp from \"../Components/WeatherApp\";\nimport Title from \"../Components/Title\";\nimport Article from \"../Components/Article\";\n// Grid //\nimport GridTemplate from \"../Components/Grid/GridTemplate\";\nimport HeroMain from \"../Components/Grid/HeroMain\";\nimport SubMain from \"../Components/Grid/SubMain\";\nimport SubAlt from \"../Components/Grid/SubAlt\";\nimport MostViewed from \"../Components/Grid/MostViewed\";\n\nimport { dateFormatter } from \"../Functions\";\n\n// STYLES\nimport styled from \"styled-components\";\nimport { col } from \"../Styles/Styles\";\n// import { titleAnim } from \"../Animations/animation\";\n\nconst Home = () => {\n  // API CALLS\n  const dispatch = useDispatch();\n  useEffect(() => {\n    dispatch(loadSection(false));\n  }, [dispatch]);\n\n  const { sectionData, loading } = useSelector((state) => state.guardian);\n\n  // TITLE COMPONENT DATA\n  let date = dateFormatter();\n  const titleData = {\n    mainTitle: \"Headlines\",\n    subTitle: date,\n  };\n  const location = useLocation();\n  const rootPath = \"/\";\n  const articlePath = location.pathname.split(\"/\")[2];\n\n  return (\n    <div className=\"news\">\n      {articlePath && <Article rootPath={rootPath} sectionData={sectionData} />}\n      <GridTemplate\n        left={<Title titleData={titleData} />}\n        // weatherApp={<WeatherApp />}\n        right={<HeroMain newsData={sectionData} loading={loading} />}\n      />\n      <GridTemplate\n        right={<SubMain newsData={sectionData} loading={loading} />}\n      />\n      <div className=\"bg--alt\">\n        <GridTemplate\n          left={false}\n          right={<SubAlt newsData={sectionData} loading={loading} />}\n        />\n      </div>\n      <GridTemplate\n        left={<h2 className=\"left-col__title\">Most Viewed</h2>}\n        right={<MostViewed newsData={sectionData} loading={loading} />}\n      />\n    </div>\n  );\n};\n\nconst HeadingBg = styled.section`\n  width: 100%;\n`;\n\nexport default Home;\n","/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Nav/SideNav.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Nav/MenuLinks.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Nav/SubNav.js",["167"],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/WeatherApp.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Functions.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Styles/Styles.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Redux/Actions/guardianSectionAction.js",["168"],"import axios from \"axios\";\nimport { guardianSection, searchAll } from \"../../api\";\n\nexport const loadSection = (section) => async (dispatch) => {\n  try {\n    const sectionData =\n      section == false\n        ? await axios.get(searchAll())\n        : await axios.get(guardianSection(section));\n    dispatch({\n      type: \"FETCH_SECTION\",\n      payload: { sectionData: sectionData.data.response.results },\n    });\n  } catch {\n    console.log(\"error\");\n  }\n};\n","/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Grid/SubMain.js",["169","170"],"// REACT\nimport React from \"react\";\n\nimport ArticlePrimary from \"../Articles/ArticlePrimary\";\nimport ArticleSingle from \"../Articles/ArticleSingle\";\n\nconst SubMain = (props) => {\n  const { newsData, loading } = props;\n  return (\n    <>\n      {!loading && (\n        <div className=\"grid grid--3fr-1fr\">\n          <div className=\"grid grid--3fr\">\n            {newsData.map((article, index) =>\n              index === 5 ? <ArticlePrimary article={article} /> : \"\"\n            )}\n\n            <div className=\"article-wrapper--column\">\n              {newsData.map((article, index) => {\n                if (index > 4 && index < 8) {\n                  return <ArticleSingle img={false} article={article} />;\n                }\n              })}\n            </div>\n          </div>\n          {/* RIGHT COLUMN */}\n          <div className=\"article-wrapper--column\">\n            {newsData.map((article, index) => {\n              if (index > 7 && index < 11) {\n                return <ArticleSingle img={false} article={article} />;\n              }\n            })}\n          </div>\n        </div>\n      )}\n    </>\n  );\n};\n\nexport default SubMain;\n","/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Grid/MostViewed.js",["171","172"],"import React from \"react\";\n\nconst MostViewed = (props) => {\n  const { newsData, loading } = props;\n  return (\n    <>\n      {!loading && (\n        <div className=\"most-viewed\">\n          <ul className=\"grid__item--span1 most-viewed__list--left\">\n            {newsData.map((entry, index) => {\n              if (index >= 0 && index < 5) {\n                return (\n                  <li className=\"most-viewed__list-item\">\n                    <p className=\"most-viewed__index-heading\">{index + 1} </p>\n                    <h4 className=\"most-viewed__heading\">\n                      {entry.fields.headline}\n                    </h4>\n                  </li>\n                );\n              }\n            })}\n          </ul>\n          <ul className=\"grid__item--span1  most-viewed__list--right\">\n            {newsData.map((entry, index) => {\n              if (index >= 5 && index < 11) {\n                return (\n                  <li className=\"most-viewed__list-item\">\n                    <p className=\"most-viewed__index-heading\">{index + 1} </p>\n                    <h4 className=\"most-viewed__heading\">\n                      {entry.fields.headline}\n                    </h4>\n                  </li>\n                );\n              }\n            })}\n          </ul>\n        </div>\n      )}\n    </>\n  );\n};\n\nexport default MostViewed;\n","/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Grid/SubAlt.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Title.js",["173"],"import React from \"react\";\n// STYLES\nimport styled from \"styled-components\";\nimport Typography from \"@material-ui/core/Typography\";\nimport { col, fontS } from \"../Styles/Styles\";\n\nconst Title = (props) => {\n  const { mainTitle, subTitle } = props.titleData;\n\n  return (\n    <>\n      <TitleContainer>\n        <h2 className=\"left-col__title article__heading--primary\">\n          {mainTitle}\n        </h2>\n\n        {subTitle && (\n          <h6>\n            <span>{` ${subTitle[0]} ${subTitle[1]} ${subTitle[2]}`}</span>\n          </h6>\n        )}\n      </TitleContainer>\n    </>\n  );\n};\n\nconst TitleContainer = styled.div`\n  display: flex;\n  align-items: center;\n  h2 {\n    margin-right: 0.5rem;\n  }\n  h6 {\n    display: inline;\n    line-height: 1;\n    margin: 0;\n    span {\n      color: ${col.title};\n    }\n    font-weight: 900;\n    font-size: ${fontS.md};\n  }\n  @media (min-width: 960px) {\n    flex-direction: column;\n    align-items: flex-start;\n    h2 {\n      margin-bottom: 0;\n    }\n  }\n  @media (max-width: 600px) {\n    h6 {\n      display: none;\n    }\n  }\n`;\n\nexport default Title;\n","/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Grid/HeroMain.js",["174","175"],"// REACT\nimport React from \"react\";\nimport { Link, useLocation } from \"react-router-dom\";\n\nimport ArticleHero from \"../Articles/ArticleHero\";\nimport ArticleSingle from \"../Articles/ArticleSingle\";\nimport ArticleTertiary from \"../Articles/ArticleTertiary\";\n\nconst HeroMain = (props) => {\n  const { newsData, loading } = props;\n  return (\n    <>\n      {!loading && (\n        <div className=\"grid grid--3fr-1fr\">\n          <div className=\"grid grid--3fr\">\n            {newsData.map((article, index) =>\n              index === 0 ? (\n                <ArticleHero\n                  id={article.id}\n                  key={article.id}\n                  article={article}\n                />\n              ) : (\n                \"\"\n              )\n            )}\n\n            <div className=\"grid grid--3fr grid__item--span3\">\n              {newsData.map((article, index) =>\n                index > 0 && index < 4 ? (\n                  <ArticleTertiary article={article} />\n                ) : (\n                  \"\"\n                )\n              )}\n            </div>\n          </div>\n          {/* RIGHT COLUMN */}\n          <div className=\"article-wrapper--column\">\n            {newsData.map((article, index) =>\n              index === 4 ? <ArticleSingle img={true} article={article} /> : \"\"\n            )}\n          </div>\n        </div>\n      )}\n    </>\n  );\n};\n\nexport default HeroMain;\n","/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Redux/Actions/weatherAction.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/api.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Pages/Opinion.js",["176"],"// REACT\nimport React, { useEffect } from \"react\";\nimport { useLocation } from \"react-router-dom\";\n\n// REDUX\nimport { loadSection } from \"../Redux/Actions/guardianSectionAction\";\nimport { useDispatch, useSelector } from \"react-redux\";\n\n// COMPONENTS\nimport Title from \"../Components/Title\";\nimport Article from \"../Components/Article\";\n\n// Grid //\nimport GridTemplate from \"../Components/Grid/GridTemplate\";\nimport HeroMain from \"../Components/Grid/HeroMain\";\nimport SubMain from \"../Components/Grid/SubMain\";\nimport SubAlt from \"../Components/Grid/SubAlt\";\nimport MostViewed from \"../Components/Grid/MostViewed\";\n\n// STYLES\nimport styled from \"styled-components\";\n\nconst Opinion = () => {\n  // API CALLS\n  const dispatch = useDispatch();\n  useEffect(() => {\n    dispatch(loadSection(\"commentisfree\"));\n  }, [dispatch]);\n  const { sectionData, loading } = useSelector((state) => state.guardian);\n\n  // TITLE COMPONENT DATA\n  const titleData = {\n    mainTitle: \"Opinions\",\n    subTitle: false,\n  };\n  const location = useLocation();\n  const rootPath = location.pathname.split(\"/\")[1];\n  const articlePath = location.pathname.split(\"/\")[2];\n\n  return (\n    <div className=\"opinion\">\n      {articlePath && <Article rootPath={rootPath} sectionData={sectionData} />}\n      <div className=\"bg--alt\">\n        <G