    width: 100%;\n    border-radius: none;\n  }\n  @media (min-width: 960px) {\n    top: 2.5rem;\n    left: 0;\n    padding: 0.25rem;\n    display: flex;\n    box-shadow: none;\n    border-top: none;\n    background-color: transparent;\n    .forecastContainer {\n      ul {\n        li:nth-of-type(2) {\n          border-color: ${col.brandMid};\n        }\n        li > p {\n          font-size: 0.8rem;\n        }\n      }\n    }\n  }\n`;\n\nexport default WeatherApp;\n","/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Functions.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Styles/Styles.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Redux/Actions/guardianSectionAction.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Grid/SubMain.js",["244","245"],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Grid/MostViewed.js",["246","247"],"import React from \"react\";\n\nimport { Link, useLocation } from \"react-router-dom\";\n\nconst MostViewed = (props) => {\n  const { newsData, loading } = props;\n  const location = useLocation();\n  const path = location.pathname;\n  return (\n    <>\n      {!loading && (\n        <div className=\"most-viewed\">\n          <ul className=\"grid__item--span1 most-viewed__list--left\">\n            {newsData.map((entry, index) => {\n              if (index >= 0 && index < 5) {\n                return (\n                  <Link\n                    to={\n                      path === \"/\"\n                        ? `article/${entry.id}`\n                        : `${path}/article/${entry.id}`\n                    }\n                    id={entry.id}\n                    key={entry.id}\n                  >\n                    <li className=\"most-viewed__list-item\">\n                      <p className=\"most-viewed__index-heading\">{index + 1} </p>\n                      <h4 className=\"most-viewed__heading\">\n                        {entry.fields.headline}\n                      </h4>\n                    </li>\n                  </Link>\n                );\n              }\n            })}\n          </ul>\n          <ul className=\"grid__item--span1  most-viewed__list--right\">\n            {newsData.map((entry, index) => {\n              if (index >= 5 && index < 10) {\n                return (\n                  <Link\n                    to={\n                      path === \"/\"\n                        ? `article/${entry.id}`\n                        : `${path}/article/${entry.id}`\n                    }\n                    id={entry.id}\n                    key={entry.id}\n                  >\n                    <li\n                      id={entry.id}\n                      key={entry.id}\n                      className=\"most-viewed__list-item\"\n                    >\n                      <p className=\"most-viewed__index-heading\">{index + 1} </p>\n                      <h4 className=\"most-viewed__heading\">\n                        {entry.fields.headline}\n                      </h4>\n                    </li>\n                  </Link>\n                );\n              }\n            })}\n          </ul>\n        </div>\n      )}\n    </>\n  );\n};\n\nexport default MostViewed;\n","/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Grid/SubAlt.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Title.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Grid/HeroMain.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Redux/Actions/weatherAction.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/api.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Pages/Opinion.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Pages/Culture.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Pages/Sport.js",["248"],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Pages/Lifestyle.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Articles/ArticleTertiary.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Articles/ArticleSingle.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Articles/ArticleHero.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Articles/ArticlePrimary.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Articles/ArticlePrimaryAlt.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Grid/GridTemplate.js",["249","250"],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Pages/Account.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/firebase.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Redux/Reducers/authReducers.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Account/SignUpModal.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Redux/Actions/authAction.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Redux/store.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Account/LoginModal.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/ArticleDetail/ArticleDetail.js",["251","252"],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/ArticleDetail/CommentsList.js",["253","254","255"],"// REACT\nimport React, { useEffect } from \"react\";\nimport { useLocation, Link } from \"react-router-dom\";\n\n// REDUX\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { getComments } from \"../../Redux/Actions/dbAction\";\n\n// COMPONENTS\nimport Comment from \"./Comment\";\nimport CreateComment from \"./CreateComment\";\n// STYLES\nimport logo from \"../../Images/logo.png\";\n\nconst CommentsList = () => {\n  const dispatch = useDispatch();\n  const location = useLocation();\n\n  const { comments, commentsLoading } = useSelector((state) => state.comments);\n  const { currentUser, userLoading } = useSelector(\n    (state) => state.currentUser\n  );\n  // REDUX\n  const articlePath = location.pathname.split(\"/article/\")[1];\n  useEffect(() => {\n    dispatch(getComments(articlePath));\n  }, [dispatch]);\n\n  return (\n    <>\n      <div className=\"comments__container\">\n        <h2 className=\"comments__heading\">Comments</h2>\n        <CreateComment url={articlePath} />\n        {/* {currentUser.name && !userLoading ? (\n          <CreateComment url={articlePath} />\n        ) : (\n          <p className=\"comments__login-signup-p\">\n            Please <Link to=\"/login\">login</Link> or{\" \"}\n            <Link to=\"/signup\">Sign Up</Link> to leave a comment\n          </p>\n        )} */}\n        {!commentsLoading && (\n          <ul className=\"comments__list\">\n            {comments.map((comment) => {\n              return (\n                <Comment\n                  author={comment.author}\n                  comment={comment.comment}\n                  authorUid={comment.author_uid}\n                  commentUid={comment.commentUid}\n                  key={comment.commentUid}\n                  id={comment.commentUid}\n                />\n              );\n            })}\n          </ul>\n        )}\n        <div className=\"comment__footer\">\n          <Link to=\"/\">\n            <img src={logo} alt=\"The Guardian Logo\" />\n          </Link>\n        </div>\n      </div>\n    </>\n  );\n};\n\nexport default CommentsList;\n","/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Redux/Actions/dbAction.js",["256"],"// FIREBASE\nimport { db } from \"../../firebase\";\n\nconst commentsDispatch = (comments) => {\n  return {\n    type: \"GET_COMMENTS\",\n    payload: {\n      comments: comments,\n      commentsLoading: false,\n    },\n  };\n};\nconst clearCommentsDispatch = () => {\n  return {\n    type: \"CLEAR_COMMENTS\",\n    payload: {\n      comments: {},\n      commentsLoading: true,\n    },\n  };\n};\n\nexport const getComments = (articlePath) => async (dispatch) => {\n  let commentsArray = [];\n  try {\n    db.collection(\"comments\")\n      .orderBy(\"index\")\n      .where(\"url\", \"==\", articlePath)\n      .onSnapshot(\n        (snapshot) => {\n          let changes = snapshot.docChanges();\n          changes.map((change) => {\n            let commentDetails = {};\n\n            commentDetails = change.doc.data();\n            commentDetails.commentUid = change.doc.id;\n            if (change.type === \"added\") commentsArray.unshift(commentDetails);\n            else if (change.type === \"modified\") {\n              const index = commentsArray.findIndex(\n                (v) => v.commentUid === commentDetails.commentUid\n              );\n              if (index > -1) {\n                commentsArray.splice(index, index + 1);\n                commentsArray.unshift(commentDetails);\n              }\n            } else if (change.type === \"removed\") {\n              const index = commentsArray.findIndex(\n                (v) => v.commentUid === commentDetails.commentUid\n              );\n              if (index > -1) commentsArray.splice(index, index + 1);\n            }\n          });\n          dispatch(commentsDispatch(commentsArray));\n        },\n        (err) => {\n          console.log(err.message);\n        }\n      );\n  } catch {}\n};\n\nexport const clearComments = () => async (dispatch) => {\n  try {\n    dispatch(clearCommentsDispatch());\n  } catch {}\n};\n","/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Redux/Reducers/dbReducers.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/ArticleDetail/Comment.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/ArticleDetail/CreateComment.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Redux/Actions/toggleActions.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/TitleAlt.js",["257","258","259"],"import React from \"react\";\n// STYLES\nimport styled from \"styled-components\";\nimport { col, fontS } from \"../Styles/Styles\";\n\nconst TitleAlt = (props) => {\n  const { mainTitle, subTitle } = props.titleData;\n  const { url, tagline, subTag } = subTitle;\n\n  return (\n    <>\n      <TitleContainer>\n        <div className=\"maintitleContainer\">\n          <h2 className=\"left-col__title article__heading--primary\">\n            {mainTitle}\n          </h2>\n        </div>\n\n        {subTitle && (\n          <>\n            <div className=\"subtitleContainer\">\n              <div className=\"textContainer\">\n                <p>{tagline}</p>\n                <h6>{subTag}</h6>\n              </div>\n              <div className=\"imgContainer\">\n                <img src={url} alt=\"CP Scott Photo\" />\n              </div>\n            </div>\n          </>\n        )}\n      </TitleContainer>\n    </>\n  );\n};\n\nconst TitleContainer = styled.div`\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  flex-flow: row wrap;\n  padding: 0.5rem 0;\n  width: 100%;\n  .maintitleContainer {\n    justify-self: flex-start;\n  }\n  .subtitleContainer {\n    display: flex;\n    align-items: cent