","/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Grid/MostViewed.js",["253","254"],"import React from \"react\";\n\nimport { Link, useLocation } from \"react-router-dom\";\n\nconst MostViewed = (props) => {\n  const { newsData, loading } = props;\n  const location = useLocation();\n  const path = location.pathname;\n  return (\n    <>\n      {!loading && (\n        <div className=\"most-viewed\">\n          <ul className=\"grid__item--span1 most-viewed__list--left\">\n            {newsData.map((entry, index) => {\n              if (index >= 0 && index < 5) {\n                return (\n                  <Link\n                    to={\n                      path === \"/\"\n                        ? `article/${entry.id}`\n                        : `${path}/article/${entry.id}`\n                    }\n                    id={entry.id}\n                    key={entry.id}\n                  >\n                    <li className=\"most-viewed__list-item\">\n                      <p className=\"most-viewed__index-heading\">{index + 1} </p>\n                      <h4 className=\"most-viewed__heading\">\n                        {entry.fields.headline}\n                      </h4>\n                    </li>\n                  </Link>\n                );\n              }\n            })}\n          </ul>\n          <ul className=\"grid__item--span1  most-viewed__list--right\">\n            {newsData.map((entry, index) => {\n              if (index >= 5 && index < 10) {\n                return (\n                  <Link\n                    to={\n                      path === \"/\"\n                        ? `article/${entry.id}`\n                        : `${path}/article/${entry.id}`\n                    }\n                    id={entry.id}\n                    key={entry.id}\n                  >\n                    <li\n                      id={entry.id}\n                      key={entry.id}\n                      className=\"most-viewed__list-item\"\n                    >\n                      <p className=\"most-viewed__index-heading\">{index + 1} </p>\n                      <h4 className=\"most-viewed__heading\">\n                        {entry.fields.headline}\n                      </h4>\n                    </li>\n                  </Link>\n                );\n              }\n            })}\n          </ul>\n        </div>\n      )}\n    </>\n  );\n};\n\nexport default MostViewed;\n","/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Grid/SubAlt.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Title.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Grid/HeroMain.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Redux/Actions/weatherAction.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/api.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Pages/Opinion.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Pages/Culture.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Pages/Sport.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Pages/Lifestyle.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Articles/ArticleTertiary.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Articles/ArticleSingle.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Articles/ArticleHero.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Articles/ArticlePrimary.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Articles/ArticlePrimaryAlt.js",[],"/Users/rua