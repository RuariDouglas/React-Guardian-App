v>\n        </div>\n      )}\n    </>\n  );\n};\n\nexport default SubMain;\n","/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Grid/MostViewed.js",["202","203"],"import React from \"react\";\n\nconst MostViewed = (props) => {\n  const { newsData, loading } = props;\n  return (\n    <>\n      {!loading && (\n        <div className=\"most-viewed\">\n          <ul className=\"grid__item--span1 most-viewed__list--left\">\n            {newsData.map((entry, index) => {\n              if (index >= 0 && index < 5) {\n                return (\n                  <li\n                    id={entry.id}\n                    key={entry.id}\n                    className=\"most-viewed__list-item\"\n                  >\n                    <p className=\"most-viewed__index-heading\">{index + 1} </p>\n                    <h4 className=\"most-viewed__heading\">\n                      {entry.fields.headline}\n                    </h4>\n                  </li>\n                );\n              }\n            })}\n          </ul>\n          <ul className=\"grid__item--span1  most-viewed__list--right\">\n            {newsData.map((entry, index) => {\n              if (index >= 5 && index < 11) {\n                return (\n                  <li\n                    id={entry.id}\n                    key={entry.id}\n                    className=\"most-viewed__list-item\"\n                  >\n                    <p className=\"most-viewed__index-heading\">{index + 1} </p>\n                    <h4 className=\"most-viewed__heading\">\n                      {entry.fields.headline}\n                    </h4>\n                  </li>\n                );\n              }\n            })}\n          </ul>\n        </div>\n      )}\n    </>\n  );\n};\n\nexport default MostViewed;\n","/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardian-App/src/Components/Grid/SubAlt.js",[],"/Users/ruaridouglas/Documents/Coding_Studies/Projects/React-Guardia